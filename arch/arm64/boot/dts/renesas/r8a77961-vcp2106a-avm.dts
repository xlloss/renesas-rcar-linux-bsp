&csi40 {
	status = "okay";

	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@0 {
			reg = <0>;

			csi40_in: endpoint {
				clock-lanes = <0>;
				data-lanes = <1 2 3 4>;
				remote-endpoint = <&deser_out0>;
			};
		};
	};
};

&i2c2 {
	status = "okay";

	tda7786@61 {
		compatible = "tda,tda7786";
		reg = <0x61>;

		rdsint = <&gpio6 5 GPIO_ACTIVE_HIGH >;
		rstn = <&gpio5 2 GPIO_ACTIVE_LOW>;
	};

	rs5c372_rtc:rs5c372a@32 {
		compatible = "ricoh,rs5c372a";
		reg = <0x32>;
	};

	ak7738: codec@18{
		compatible = "akm,ak7738";
		reg = <0x18>;
		#sound-dai-cells = <1>;
		clocks = <&rcar_sound 3>;
		clock-names = "mclk";
		pdn-gpio = <&gpio6 13 GPIO_ACTIVE_HIGH>;
		AVDD-supply = <&reg_audio>;
		LVDD-supply = <&reg_audio>;
		DRVDD-supply = <&reg_audio>;
		VDD33-supply = <&reg_audio>;
		TVDD1-supply = <&reg_audio>;
		TVDD2-supply = <&reg_audio>;
		D3V-supply = <&reg_audio>;
		status = "okay";

		port@0 {
			ak7738_endpoint0: endpoint {
			};
		};
		port@1 {
			ak7738_endpoint1: endpoint {

			};
		};
		port@2 {
			ak7738_endpoint2: endpoint {

			};
		};
		port@3 {
			ak7738_endpoint3: endpoint {
				remote-endpoint = <&rsnd_endpoint0>;
			};
		};
		port@4 {
			ak7738_endpoint4: endpoint {

			};
		};
	};
};

&i2c4 {
	status = "okay";

};

&i2c5 {
	status = "okay";

	kx122@1e { /* Kionix accelerometer */
		compatible = "kionix,kx122";
		reg = <0x1e>;
		vdd-supply = <&reg_kx122_vdd>;
		vio-supply = <&reg_kx122_vdd>;
		interrupt-parent = <&gpio2>;
		interrupts = <3 0x1>;
		kionix,gpio-int1 = <&gpio2 3 0x1>;
		kionix,x-map = <1>;
		kionix,y-map = <2>;
		kionix,z-map = <0>;
		kionix,x-negate = <0>;
		kionix,y-negate = <0>;
		kionix,z-negate = <0>;
		kionix,g-range = <8>;
		kionix,use-drdy-int;
	};

	kx122@1f { /* Kionix accelerometer */
		compatible = "kionix,kx122";
		reg = <0x1f>;
		vdd-supply = <&reg_kx122_vdd>;
		vio-supply = <&reg_kx122_vdd>;
		interrupt-parent = <&gpio2>;
		interrupts = <3 0x1>;
		kionix,gpio-int1 = <&gpio2 3 0x1>;
		kionix,x-map = <1>;
		kionix,y-map = <2>;
		kionix,z-map = <0>;
		kionix,x-negate = <0>;
		kionix,y-negate = <0>;
		kionix,z-negate = <0>;
		kionix,g-range = <8>;
		kionix,use-drdy-int;
	};
};

